            <button type="submit" class="btn btn-primary ml-2" routerLink="/superadmin/candidate">Cancel</button>
            <button type="submit" class="btn btn-success ml-2" style="float:right">Submit</button>
            <button type="reset" class="btn btn-success ml-2" style="float:right">Reset</button>
        </form>
    </div>
</div>



<!-- Qulaification modal -->
<div class="modal fade " id="qualificatio" style="margin-top: -29px;" tabindex="-1" role="dialog"
    aria-labelledby="myExtraLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg ">
        <div class="modal-content ">
            <div class="container">
                <nav class="navbar navbar-expand-lg p-3 ">
                    <h5>Qualification</h5>
                    <div class="collapse navbar-collapse">
                        <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                        </ul>
                        <!-- <form class="form-inline my-4 my-lg-0">
                            <input type="search" placeholder="Search">
                        </form> -->
                    </div>
                    <button type="button" class="close ml-4" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </nav>
                <table class="table">
                    <!-- <thead>
                        <tr>
                            <th><b>Qualification</b></th>
                        </tr>
                    </thead> -->
                    <tbody>
                        <tr *ngFor="let o of highquali">
                            <td> <input type="radio" name="foobar" value="{{o.qualificationName}}"
                                    [(ngModel)]="selectedqual">{{o.qualificationName}}<br></td>
                        </tr>
                    </tbody>


                </table>
                <button type="button" class="btn btn-sm btn-primary mb-3" style="float: right;"
                    data-dismiss="modal">Save
                    changes</button>
            </div>
        </div>
    </div>
</div>

