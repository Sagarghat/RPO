<div id="myModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title"></h4>
            </div>
            <div class="modal-body">
                <div class=" col-sm-5 ml-5">
                    <div class="row p-0">
                        <div class="col-sm-4 p-0 my-2">
                            <label type="text" class="form-control" style=" border-bottom: 0px;">
                                Choose Job Opening</label>
                        </div>
                        <div class="col-sm-4 p-0 my-2">
                            <input type="text" name="profileDetails" class="form-control"
                                formControlName="profileDetails" [(ngModel)]="selectedjobopenings">
                            <span class="d-inline float-right" style="margin-top: -1.6rem;" data-toggle="modal"
                                data-target="#postingtitle">
                                <i class="fa fa-user-o" aria-hidden="true" data-toggle="tooltip"
                                    title="show options"></i></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<!-- jobopenings modal -->
<div class="modal fade " id="postingtitle" style="margin-top: -29px;" tabindex="-1" role="dialog"
    aria-labelledby="myExtraLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg ">
        <div class="modal-content ">
            <div class="container">
                <nav class="navbar navbar-expand-lg p-3 ">
                    <h5>Posting Title </h5>
                    <div class="collapse navbar-collapse">
                        <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                        </ul>
                        <!-- <form class="form-inline my-4 my-lg-0">
                            <input type="search" placeholder="Search"><i class="fas fa-search"></i>
                        </form> -->
                    </div>
                    <button type="button" class="close ml-4" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </nav>
                <table class="table">
                    <thead>
                        <th></th>
                        <th>JOB OPENING ID</th>
                        <th>POSTING TITLE</th>
                        <th>TARGET DATE</th>
                        <th>JOB OPENING STATUS</th>
                        <th>CLIENT NAME</th>
                        <th> CONTACT NAME</th>
                        <th>JOB OPENING OWNER</th>
                    </thead>
                    <tbody>
                        <tr *ngFor="let c of totaljob">
                            <td>
                                <input type="radio" [value]="c.nameOfRequirement" [(ngModel)]="selectedPostingTitle">
                            </td>
                            <td>{{c.id}}</td>
                            <td>{{c.nameOfRequirement}}</td>
                            <td>{{c.requirementEndDate|date:"dd-MM-yyyy"}}</td>
                            <td>{{c.requirementStatus}}</td>
                            <td>{{c.clientName}}</td>
                            <td>{{c.contactEmail}}</td>
                        </tr>
                    </tbody>
                </table>
                <button type="button" class="btn btn-sm btn-primary mb-3" style="float: right;"
                    data-dismiss="modal">Savechanges</button>
                <button type="button" class="btn btn-sm btn-success mb-3" style="float: right;"
                    data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>